
generator client {
  provider = "prisma-client-js"
  output        = "../generated/client" //Change output to custom path
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}


model UserPreference{
  Id  Int @id @default(autoincrement())
  email String @unique
  name String
  ImageUrl String 
  age Int
  university String
  starting DateTime
  ending DateTime
  language String
  major String
  country String
  foodPrefernce String
  smoking String
  drinking String
  expenditure String
  ambiance String
  tidiness String
  socializing String
  about String
  findmate Boolean @default(false)
  timstamp DateTime @default(now())
  }